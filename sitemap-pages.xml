    <priority>{{ p.priority }}</priority>
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/en{{ p.url }}" />
    <xhtml:link rel="alternate" hreflang="de" href="{{ site.url }}{{ p.url }}" />
  </url>
  <url>
    <loc>{{ site.url }}/en{{ p.url }}</loc>
    <priority>{{ p.priority }}</priority>
    <xhtml:link rel="alternate" hreflang="de" href="{{ site.url }}{{ p.url }}" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/en{{ p.url }}" />
  </url>
{% endfor %}
</urlset>
